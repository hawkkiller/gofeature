{
  "name": "gofeature",
  "displayName": "gofeature",
  "description": "Helpful Golang snippets and other things",
  "version": "1.0.1",
  "publisher": "MykhailoLazebnyi",
  "repository": {
    "type": "git",
    "url": "https://github.com/hawkkiller/gofeature"
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "go",
    "snippets",
    "feature"
  ],
  "icon": "assets/logo.png",
  "activationEvents": [
    "onCommand:extension.new-feature",
    "workspaceContains:**/go.mod"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.new-feature",
        "title": "GoFeature: New Feature",
        "icon": "assets/logo.png"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.new-feature",
          "group": "featureGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "go",
        "path": "./snippets/go.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.121",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^2.2.42",
    "@types/node": "^12.7.8",
    "@types/node-fetch": "^2.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^6.0.2",
    "@types/vscode": "^1.56.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.12.1",
    "typescript": "^3.5.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.0.0",
    "semver": "^6.3.0"
  }
}
